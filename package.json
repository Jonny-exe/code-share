  "bugs": {
    "url": "https://github.com/Jonny-exe/code-share/issues"
  },
  "homepage": "https://github.com/Jonny-exe/code-share#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "husky": "^4.3.7",
    "live-server": "^1.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
